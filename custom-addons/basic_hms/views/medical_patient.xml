        <field name="view_mode">tree,form</field>
        <field name="view_id" ref="account.view_move_tree"/>
        <field name="domain">[('move_type', '=', 'out_invoice')]</field>
    </record>
    <record id="view_task_form_inherited" model="ir.ui.view">
        <field name="name">project.task.form.inherited</field>
        <field name="model">project.task</field>
        <field name="inherit_id" ref="project.view_task_form2"/>
        <field name="arch" type="xml">
            <xpath expr="//group//field[@name='project_id']" position="after">
                <field name="doctor_id" string="Doctor Name"/>
                <field name="patient_id" string="Patient Name"/>
            </xpath>
            <!-- Modifying project_id field -->
            <xpath expr="//group//field[@name='project_id']" position="attributes">
                <attribute name="string">Treatment Type</attribute>
            </xpath>
            <xpath expr="//page[@name='extra_info']" position="after">
                <page name="build_info" string="Fabrication">
                    <group>
                        <field name="product_id"/>
                        <field name="bom_id"/>
                        <field name="quantity"/>

                    </group>
                    <button string="Create Fabrication Order" type="object" name="create_manufacturing_order"
                            class="oe_highlight"/>
                    <div style="display:inline-block; width: 10px;"></div>
                    <button string="Cancel" class="btn-secondary" special="cancel" style="padding:10px"/>
                </page>
            </xpath>
        </field>
    </record>
