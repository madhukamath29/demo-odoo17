<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="medical_patients_form_view" model="ir.ui.view">
        <field name="name">medical.patients.form.view</field>
        <field name="model">medical.patient</field>
        <field name="arch" type="xml">

            <form string="Patient">
                <header>
                    <button name="print_report" type="object"
                            string="Print Patient Card" class="oe_highlight"/>
                    <button name="action_view_prescriptions" type="object" string="View Prescriptions"
                            class="oe_highlight"/>
                    <button name="action_create_project_task" type="object" string="Create Treatment Plan"
                            class="oe_highlight"/>
                    <button name="action_create_appointment" type="object" string="Create Appointment"
                            class="oe_highlight"/>
                    <button name="action_create_lab_test" type="object" string="Create Lab Test" class="oe_highlight"/>

                </header>
                <sheet>
                    <div class="oe_button_box" name="button_box">
                        <button name="action_view_patient_tasks" type="object" class="oe_stat_button" icon="fa-tasks">
                            <field name="task_count" widget="statinfo" string="Treatment Plans"/>
                        </button>
                        <button name="action_view_appointment" type="object" class="oe_stat_button"
                                icon="fa-calendar">
                            <field name="appointment_count" widget="statinfo" string="Appointments"/>
                        </button>
                        <button name="action_view_invoices" type="object" class="oe_stat_button" icon="fa-file-text">
                            <field name="invoice_count" widget="statinfo" string="Invoices"/>
                        </button>
                        <button name="action_view_tooth_report" type="object" class="oe_stat_button" icon="fa-tasks">
                            <field name="appointment_count" widget="statinfo" string="Tooth Reports"/>
                        </button>
                        <!--                        <field name="invoice_count" widget="statinfo" string="Invoices"/>-->
                    </div>

                    <field name="photo" nolabel="1"
                           widget="image" class="oe_avatar"
                    />

                    <div class="oe_title mb24">
                        <div class="oe_edit_only">
                            <label for="patient_id"/>
                        </div>
                        <h1>
                            <field name="patient_id"
                                   default_focus="1" placeholder="Patient Name"
                                   context="{'default_is_patient': 1,'default_is_person': 1}"
                            />
                        </h1>
                    </div>

                    <group>
                        <group>
                            <field name="name"/>
                            <field name="date_of_birth"/>
                            <field name="occupation"/>
                            <field name="mobile"/>
                            <field name="govt_id_type"/>
                            <field name="govt_id" modifiers="{'invisible': [('govt_id_type', '=', False)]}"
                                   options='{"no_label": True}'/>
                            <!--                            <label for="govt_id" string=""/>-->
                            <!--                            <field name="diagnosis" placeholder="Ex: Root Infection, Enamel Erosion"/>-->
                            <field name="language_preferences"/>
                            <field name="preferred_appointment_times"/>
                            <!--                            <field name="special_needs_or_disabilities"/>-->
                            <!--                            <field name="anxiety_or_phobia_information"/>-->

                            <field name="treatment_consent"/>
                            <field name="treatment_consent_label" readonly="1" nolabel="1"/>
                        </group>

                        <group>
                            <field name="sex"/>
                            <field name="age" readonly="1"/>
                            <field name="partner_address_id" invisible="1"/>
                            <label for="partner_address_id" string="Address" name="partner_address_id"/>
                            <div class="o_address_format">
                                <field name="street" placeholder="Street..." class="o_address_street"/>
                                <field name="street2" placeholder="Street 2..." class="o_address_street"/>
                                <field name="city" placeholder="City" class="o_address_city"/>
                                <field name="state_id" class="o_address_state" placeholder="State"
                                       options="{&quot;no_open&quot;: True}"/>
                                <field name="zip_code" placeholder="ZIP" class="o_address_zip"/>
                                <field name="country_id" placeholder="Country" class="o_address_country"/>
                            </div>
                            <!--                            <field name="followUp_date"/>-->
                            <!--                            <field name="patient_ins" placeholder="Ex: Consume only cold food"/>-->
                            <field name="recall_reminders_method"/>
                            <field name="recall_reminders_frequency"/>
                            <field name="email_address"/>
                            <!--                            <field name="family_med_history"/>-->
                            <field name="pay_method_pref"/>
                        </group>
                    </group>

                    <group>
                        <separator colspan="4"
                                   string="Patient Critical Information"/>
                        <newline/>
                        <field colspan="2" name="critical_info"
                               nolabel="1"/>
                    </group>
                    <notebook>
                        <page string="General Information">
                            <group string="Patient General Information">
                                <group>
                                    <field name="blood_type"/>
                                    <field name="current_insurance_id"/>
                                    <field name="height"/>
                                    <field name="weight"/>
                                </group>
                                <group>
                                    <field name="rh"/>
                                    <field name="receivable"/>
                                    <field name="primary_care_physician_id"/>
                                </group>
                            </group>

                            <group string="Emergency Contact Details">
                                <group>
                                    <field name="relation_name"/>
                                    <field name="emergency_no"/>
                                    <field name="relation_patient"/>

                                </group>
                            </group>
                            <group colspan="4"
                                   string="Patient Extra Information">
                                <field colspan="4" name="general_info"
                                       nolabel="1"/>
                                <newline/>
                                <group>
                                    <field name="deceased"/>
                                    <field name="date_of_death"
                                           invisible="not deceased" required="deceased == 1"/>
                                    <field name="cause_of_death"
                                           invisible="not deceased"/>
                                </group>
                            </group>
                        </page>

                        <page string="Diseases">
                            <field name="patient_disease_ids"
                                   nolabel="1" widget="section_and_note_one2many"/>
                        </page>


