<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!--STANDARD Layout-->
    <!--Inherited the standard external layout for changing company address position-->
    <template id="custom_external_layout_standard" inherit_id="web.external_layout_standard">
        <xpath expr="//div/div[1]" position="replace" style="margin: 0; padding: 0;">
            <t t-if="model == 'account.move'">
                <div class="row">
                    <div t-if="company.logo" class="col-6 text-start">
                        <img t-if="company.logo" t-att-src="image_data_uri(company.logo)"
                             style="max-height:60px; max-width:160%;" alt="Logo"/>
                    </div>
                </div>

                <div class="float-right text-end" style="position: absolute; top: 0; right: 0;">
                    <!--                    <t t-if="model == 'account.move'">-->
                    <t t-if="company.is_company_details_empty">
                        <h3>
                            <div>
                                <strong t-field="company.partner_id.display_name"/>
                            </div>
                        </h3>

                        <div>
                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="company.partner_id.street"/>

                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="company.partner_id.street2"/>

                            <br/>
                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="company.partner_id.city"/>

                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="company.partner_id.state_id"/>

                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="company.partner_id.zip"/>

                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="company.partner_id.country_id"/>
                        </div>
                    </t>
                    <t t-else="">
                        <h5>
                            <div>
                                <strong t-field="res_company.name"/>
                            </div>
                        </h5>

                        <div>
                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="res_company.street"/>

                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="res_company.street2"/>

                            <br/>
                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="res_company.city"/>

                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="res_company.state_id"/>

                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="res_company.zip"/>

                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="res_company.country_id"/>
                        </div>
                    </t>
                    <!--                    </t>-->
                </div>
            </t>
            <t t-else="">
                <div class="row">
                    <div t-if="company.logo" class="col-6 text-start">
                        <img t-if="company.logo" t-att-src="image_data_uri(company.logo)"
                             style="max-height:60px; max-width:160%;" alt="Logo"/>
                    </div>
                </div>

                <div class="float-right text-end" style="position: absolute; top: 0; right: 0;">
                    <!--                    <t t-if="model == 'account.move'">-->
                    <t t-if="company.is_company_details_empty">
                        <h3>
                            <div>
                                <strong t-field="company.partner_id.display_name"/>
                            </div>
                        </h3>

                        <div>
                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="company.partner_id.street"/>

                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="company.partner_id.street2"/>

                            <br/>
                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="company.partner_id.city"/>

                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="company.partner_id.state_id"/>

                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="company.partner_id.zip"/>

                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="company.partner_id.country_id"/>
                        </div>
                    </t>
                    <t t-else="">
                        <h5>
                            <div>
                                <strong t-field="res_company.name"/>
                            </div>
                        </h5>

                        <div>
                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="res_company.street"/>

                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="res_company.street2"/>

                            <br/>
                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="res_company.city"/>

                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="res_company.state_id"/>

                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="res_company.zip"/>

                            <span t-attf-style="color:black; font-weight:300;" class="inline-text"
                                  t-field="res_company.country_id"/>
                        </div>
                    </t>
                    <!--                    </t>-->
                </div>
            </t>
        </xpath>
    </template>

